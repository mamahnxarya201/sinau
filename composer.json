    "require": {
        "dentro/yalr": "1.0.x-dev",
        "ext-json": "*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.3.5",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3",
        "laravel/octane": "^1.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "lorisleiva/laravel-actions": "^2.1",
        "monolog/monolog": "^2.3",
        "php": "^8.0",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.4",
        "slim/slim": "^4.8",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "jangregor/phpstan-prophecy": "^0.8.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.94",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symplify/monorepo-builder": "9.3.27"
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "packages/logawa/src/",
                "packages/tumapel/app/"
            ],
            "Database\\Factories\\": "packages/tumapel/database/factories/",
            "Database\\Seeders\\": "packages/tumapel/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": [
                "packages/logawa/tests/",
                "packages/tumapel/tests/"
            ]
        }
    },
    "extra": {
        "laravel": []
    },
    "replace": {
        "dentro/nge.noob": "self.version",
        "slim/slim-skeleton": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
